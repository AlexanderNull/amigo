<!-- START template: header_search.tmpl -->
<!-- page_name          = [% page_name %] -->

<div id="gsf01">
  <form action="[% base %]/amigo/search" id="query-form" method="GET">
    <p>
      <label for="bar-query">Quick search GO </label>
      <input type="text"
	     title="Hint: add a space after completing a word to narrow the search."
	     class="textBox"
	     name="bar-query"
	     size="35"
	     value=""
	     id="gsf-query" />
      
      <input type="submit"
	     title="Search for annotations with the inputted text."
	     class="button" 
	     value="Search" />
      
      [% rollup_id = "gsf01" %]
      [% rollup_anchor = "Information about this autocomplete" %]
      [% INCLUDE "common/info_rollup_start.tmpl" %]
      
      [% INCLUDE "common/autocomplete_explanation.tmpl" %]
      
      [% INCLUDE "common/info_rollup_end.tmpl" %]
      
    </p>
  </form>
</div>
<!-- END template: header_search.tmpl -->
