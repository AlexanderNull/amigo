	    [%   a2_search_desc = a2_search_blob.description %]
	    [%   a2_search_link = a2_search_blob.amigo_interlink %]
	    <li>
	      <!-- <a href="[% a2_search_link %]" -->
	      <!--    title="[% a2_search_desc %]">[% a2_search_name %]</a> -->
